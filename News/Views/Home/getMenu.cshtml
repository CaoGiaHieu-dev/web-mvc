@model List< News.Models.menu>



    @foreach (var i in Model)
    {
        if(i.id_m ==1)
        {
            <li>
                <a href="/@i.meta">@i.name</a>
            </li>
        }
        else
        {
            <li>
                <a href="~/@ViewBag.meta/@i.meta">@i.name</a>
            </li>
            }
    }

